@model ProyectoFDI.v2.Models.DetalleCompetencium

@{
    ViewData["Title"] = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
</head>

<h1>DETALLES</h1>

<div>
    <h4>Detalle de la Competencia</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            Puesto
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Puesto)
        </dd>
        <dt class = "col-sm-2">
            Resultado de Clasificación
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.ClasRes)
        </dd>
        <dt class = "col-sm-2">
            Resultado de la Final
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.FinalRes)
        </dd>
        <dt class = "col-sm-2">
            Competencia
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.IdComNavigation.NombreCom)
        </dd>
        <dt class = "col-sm-2">
            Deportista
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.IdDepNavigation.NombresDep) @Html.DisplayFor(model => model.IdDepNavigation.ApellidosDep)
        </dd>
    </dl>
</div>
<br />
<div>
    <button type="button" class="btn btn-outline-warning"><a class="boton" asp-action="Edit" asp-route-id="@Model?.IdDetalle">Editar</a></button>
    <button type="button" class="btn btn-outline-danger"><a class="boton" asp-action="Index">Regresar</a></button>
</div>

