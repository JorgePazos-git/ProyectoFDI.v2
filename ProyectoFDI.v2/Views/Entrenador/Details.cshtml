@model ProyectoFDI.v2.Models.Entrenador

@{
    ViewData["Title"] = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
</head>
<h1>DETALLES</h1>

<div>
    <h4>Entrenador</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            Nombres
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.NombresEnt)
        </dd>
        <dt class="col-sm-2">
            Apellidos
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.ApellidosEnt)
        </dd>
        <dt class="col-sm-2">
            Cedula
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.CedulaEnt)
        </dd>
        <dt class="col-sm-2">
            Provincia
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.IdProNavigation.NombrePro)
        </dd>
        <dt class="col-sm-2">
            Usuario
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.IdUsuNavigation.NombreUsu)
        </dd>
    </dl>
</div>
<br />
<div>
    <button type="button" class="btn btn-outline-warning"><a class="boton" asp-action="Edit" asp-route-id="@Model?.IdEnt">Editar</a></button>
    <button type="button" class="btn btn-outline-danger"><a class="boton" asp-action="Index">Regresar</a></button>
</div>
